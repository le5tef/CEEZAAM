{
  "name": "ui-sezam",
  "version": "1.0.0",
  "private": true,
  "author": "Sergei Kornienko <qluqua@gmail.com>",
  "engines": {
    "node": ">=16.15.1"
  },
  "scripts": {
    "dev": "vite -l info",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "cert": "cd cert && chmod +x makeCert.sh && ./makeCert.sh && sudo chmod -R 755 . && cd ..",
    "generate-client": "openapi --input ./openapi.json --output ./src/client --client axios"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.2.4",
    "@element-plus/icons-vue": "^2.0.10",
    "apexcharts": "^3.36.3",
    "axios": "^0.27.2",
    "current-device": "^0.10.2",
    "dayjs": "^1.11.6",
    "element-plus": "^2.2.17",
    "js-cookie": "^3.0.1",
    "lodash.shuffle": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "nice-try": "^3.0.1",
    "pinia": "^2.0.14",
    "sass": "^1.55.0",
    "stylus-mixins": "https://github.com/qluqua/stylus-mixins",
    "unplugin-element-plus": "^0.4.1",
    "validator": "^13.7.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.13",
    "vue3-apexcharts": "^1.4.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/google.maps": "^3.49.2",
    "@types/js-cookie": "^3.0.2",
    "@types/jsdom": "^16.2.14",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^16.11.41",
    "@types/validator": "^13.7.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "dotenv": "^16.0.3",
    "eslint": "8.22.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.0.0",
    "fs": "^0.0.1-security",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.23.0",
    "path": "^0.12.7",
    "pinia-logger": "^1.3.6",
    "prettier": "^2.5.1",
    "protobufjs": "^6.11.3",
    "stylus": "^0.58.1",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^2.9.12",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vitest": "^0.15.1",
    "vue-tsc": "^1.0.5"
  }
